        <type></type>
        <definition>addWriter</definition>
        <argsstring>($writer, $priority=null, $comparison= &apos;=&apos;)</argsstring>
        <name>addWriter</name>
        <param>
          <declname>$writer</declname>
        </param>
        <param>
          <declname>$priority</declname>
